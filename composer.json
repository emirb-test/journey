{
  "description": "Boarding pass sorter.",
  "type": "project",
  "require-dev": {
    "phpunit/phpunit": "^7.1"
  },
  "require": {
    "php": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "Acme\\": "src/Acme/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Acme\\Tests\\": "tests/Acme/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "authors": [
    {
      "name": "Emir Beganovic",
      "email": "emir@php.net"
    }
  ]
}
